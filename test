if [ $1 ];then

if [ ! -f "$1.yuv" ];then
echo converting jpeg to yuv
../ffmpeg/ffmpeg-4.2.1-win64-static/bin/ffmpeg.exe -i $1.jpg -pix_fmt yuvj420p $1.yuv
else
echo $1.yuv already exist!
fi
./yuvtrans.exe 1729 2448 $1.yuv $2 $3
../ffmpeg/ffmpeg-4.2.1-win64-static/bin/ffmpeg.exe -s 1729x2448 -pix_fmt yuvj420p -i $1_out.yuv -frames 1 -f image2 -y $1_out.jpeg
echo $1 $2 $3 done

else
echo Please input filename of input!
fi
